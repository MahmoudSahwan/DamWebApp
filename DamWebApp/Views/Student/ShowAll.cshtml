<!--Strong Type View -->
<table class="table table-hover table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Image</th>
    </tr>
    
    @foreach (Student item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.ImageUrl</td>
            <td>
                <img src="~/Images/@item.ImageUrl" style="width:100px"/> 
            </td>
        </tr>
    }
</table>


